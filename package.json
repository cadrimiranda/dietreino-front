{
  "name": "dietreino-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --no-cache",
    "test:coverage": "jest --coverage --coverageDirectory=coverage --no-cache"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.0",
    "@types/axios": "^0.14.0",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^5.14.0",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "use-http": "^1.0.28"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(png|webp)$": "<rootDir>/src/mocks/imageMock.ts"
    },
    "testEnvironment": "jsdom"
  }
}
